{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "paths": {
      "@server*" :["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
