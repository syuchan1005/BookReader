{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "paths": {
      "@server/*" :["src/*"],
      "natural-orderby": ["src/sort"]
    }
  },
  "exclude": ["node_modules"]
}
