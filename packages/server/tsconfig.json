{
  "compilerOptions": {
    "lib": ["es2020", "dom"],
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "paths": {
      "@server/*" :["src/*"],
      "natural-orderby": ["src/sort"],
      "@syuchan1005/book-reader-graphql": ["generated/GQLResolvers.ts"]
    }
  },
  "include": ["src/*"],
  "exclude": ["node_modules"]
}
